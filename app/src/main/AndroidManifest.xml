    package="com.masudin.omahkamerasragen">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Penyewaan_kamera">
        <activity android:name=".ui.profile.ProfileActivity"></activity>
        <activity android:name=".ui.product.ProductActivity" />
        <activity android:name=".ui.history_transaction.HistoryTransactionActivity" />
